{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "plugin:@next/next/recommended"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-var": "off",
    "@typescript-eslint/no-unused-vars": [
      "error"
    ],
    "@typescript-eslint/no-empty-function": "off"
  }
}